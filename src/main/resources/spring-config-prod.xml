<?xml version="1.0" encoding="UTF-8"?>
<beans profile="prod" xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user" class="spring.profileAnnotation.configurationAndXml.User">
        <constructor-arg type="java.lang.Integer" value="100"/>
        <constructor-arg type="java.lang.String" value="Prod User"/>
    </bean>
</beans>